  "dependencies": {
    "@bowtie/api": "^0.3.0",
    "@bowtie/react-utils": "^0.1.1",
    "@bowtie/utils": "^0.3.0",
    "airbrake-js": "^1.1.2",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.0.1",
    "eventemitter2": "^5.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "libphonenumber-js": "^1.2.15",
    "moment": "^2.22.2",
    "node-sass": "^4.7.2",
    "object-assign": "4.1.1",
    "parse-link-header": "^1.0.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "pubnub": "^4.21.0",
    "qs": "^6.5.2",
    "raf": "3.4.0",
    "rc-time-picker": "^3.3.1",
    "react": "^16.2.0",
    "react-ace": "^6.1.3",
    "react-color": "^2.14.1",
    "react-datepicker": "^1.5.0",
    "react-datetime": "^2.14.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.11",
    "react-pdf": "^3.0.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "reactstrap": "^6.1.0",
    "recompose": "^0.27.1",
    "sass-loader": "^6.0.6",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "timers": "^0.1.1",
    "url-loader": "0.6.2",
    "uuid": "^3.2.1",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-jest": "^22.4.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "jest": "^22.4.0",
    "jest-cli": "^22.4.0",
    "jest-enzyme": "^6.0.2",
    "nsp": "^3.2.1",
    "react-test-renderer": "^16.2.0",
    "standard": "^11.0.1"
  },
  "scripts": {
    "preinstall": "node -e \"if (process.env.npm_execpath.indexOf('yarn') === -1) { console.log('\u001b[31mPlease use yarn for installing\u001b[0m'); process.exit(1); }\"",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "npm run lint && node scripts/test.js --env=jsdom",
    "scan": "nsp check",
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.worker.js": "<rootDir>/__mocks__/workerMock.js"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
